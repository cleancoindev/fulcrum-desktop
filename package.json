    "inquirer": "^0.11.0",
    "lodash": "^3.10.1",
    "minidb": "zhm/minidb#master",
    "mixmatch": "0.0.2",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.7",
    "request": "^2.67.0",
