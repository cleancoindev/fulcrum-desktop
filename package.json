  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.4.1",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.4.0",
    "mocha": "^2.3.4",
    "should": "^8.1.1",
    "source-map-support": "^0.4.0",
