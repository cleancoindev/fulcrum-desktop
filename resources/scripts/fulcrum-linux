#!/usr/bin/env ../fulcrum-sync

require('../resources/app.asar/node_modules/source-map-support').install();
require('../resources/app.asar/node_modules/babel-register')({
  sourceMaps: 'inline',
  sourceType: 'module',
  resolveModuleSource: (source, filename) => {
    if (source === 'fulcrum') {
      return 'fulcrum-sync-plugin';
    }
    return source;
  }
});
require('../resources/app.asar/app/main/commands/cli');
